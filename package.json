    "@types/express-fileupload": "^1.4.4",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "eslint": "^8.56.0",
    "eslint-plugin-import": "^2.29.1",
    "ncp": "^2.0.0",
    "prettier": "^3.2.5",
